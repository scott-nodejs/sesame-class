.form_container{box-shadow:0 14rpx 40rpx 0 rgba(0,0,0,.08);width:686rpx;background:#fff;border-radius:12rpx;padding:0rpx 36rpx 36rpx 36rpx;box-sizing:border-box}.form_container .form_item{width:100%;height:120rpx;display:flex;flex-direction:row;justify-content:start;align-items:center;border-bottom:1rpx solid #f1f1f1;position:relative}.form_container .form_item .form_label{font-weight:400;letter-spacing:2rpx;font-size:36rpx;color:#303030;width:200rpx}.form_container .form_item .placeholder{font-weight:400;letter-spacing:2rpx;font-size:36rpx;color:#ddd}.form_container .form_item .form_input{font-weight:400;letter-spacing:2rpx;font-size:36rpx;color:#303030}.form_container .form_item .select_arrow{width:40rpx;height:40rpx;position:absolute;right:0;top:50%;-webkit-transform:translateY(-20rpx);transform:translateY(-20rpx)}.form_container .form_item .picker{width:300rpx}.form_container .form_btn{width:612rpx;height:92rpx;font-weight:400;letter-spacing:2rpx;font-size:36rpx;color:#fff;display:flex;flex-direction:row;justify-content:center;align-items:center;background:#2f54eb;box-shadow:0 14rpx 40rpx 0 #adc6ff;border-radius:12rpx;line-height:92rpx;margin-top:60rpx}.form_container .form_btn_hover{box-shadow:none}.notice{font-weight:400;letter-spacing:2rpx;font-size:36rpx;color:#aaa;margin-top:60rpx;width:686rpx}.create_page{padding:20rpx 0rpx;display:flex;flex-direction:column;justify-content:start;align-items:center}.create_page .press-canvas{position:absolute;top:-1000rpx;background-color:grey}.create_page .image{width:352rpx;height:236rpx;margin-bottom:50rpx}.create_page .selected_image{width:80rpx;height:80rpx;border-radius:12rpx;box-shadow:0 14rpx 40rpx 0 rgba(0,0,0,.08)}.create_page .notice{font-weight:400;letter-spacing:2rpx;font-size:32rpx;color:#aaa}.create_page .attention_txt{color:#2f54eb}